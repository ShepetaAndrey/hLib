{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:vue/essential"],
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "quotes": ["warn", "single"],
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "no-unused-vars": "warn",
    "no-console": "warn",
    "semi": "off",
    "eol-last": "error"
  }
}
